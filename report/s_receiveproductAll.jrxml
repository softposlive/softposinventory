	<queryString>
		<![CDATA[SELECT recive.R_PCode as pcode, product.Pdesc as pdesc ,SUM( recive.R_Qty ) as qty, SUM( recive.R_Amount ) as amount, hrecive.R_No, hrecive.R_Date, hrecive.R_Bran, recive.R_No,product.Punit1 as unit
FROM (hrecive  INNER JOIN recive
   ON  hrecive.R_No = recive.R_No )
   INNER JOIN product on recive.R_PCode = product.Pcode
WHERE   recive.R_Post = 'Y' $P!{condition}
GROUP BY recive.R_PCode
ORDER BY recive.R_PCode]]>
	</queryString>
